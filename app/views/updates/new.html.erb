<%= render "teachers/tenavbar" %>
<div class="upnewcont">
	<h1 class="mb-4">New updates</h1>
	<%= form_with model: @update do |form| %>

		<div class="field form-group mb-4">
			<%= form.label :title %><br>
			<%= form.text_field :title, class:"form-control",placeholder:"Title"  %>
		</div>

		<div class="field form-group mb-2">
			<%= form.label :body %><br>
			<%= form.text_area :body, class:"form-control",placeholder:"Body"  %>
		</div>
		<br>
		<div class="actions">
			<%= form.submit class:'btn btn-secondary'  %>
		</div>
	<% end %>
</div>